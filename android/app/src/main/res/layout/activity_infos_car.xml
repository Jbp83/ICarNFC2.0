        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="365dp" />

    <TextView
        android:id="@+id/puissance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/marque"
        android:text="Puissance :"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="411dp" />

    <TextView
        android:id="@+id/dateimmat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/modele"
        android:layout_centerHorizontal="true"
        android:text="Date d'immatroculation :"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="451dp" />

    <TextView
        android:id="@+id/result123"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gjdy"
        android:layout_centerHorizontal="true"
        android:text="Date d'immatroculation :"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="451dp" />

