<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jb.icarnfc.Infos_car"
    android:weightSum="1"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="5dp"
        android:text="Plaque d'immatriculation : "
        android:textAlignment="center" />

    <ImageView
        android:id="@+id/photo"
        android:layout_width="183dp"
        android:layout_height="183dp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="25dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp" />


    <TextView
        android:id="@+id/tv_immatriculation"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="19dp"
        android:layout_toEndOf="@+id/tv_imatValue"
        android:text="value"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="297dp" />

    <TextView
        android:id="@+id/tv_imatValue"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="Puissance : "
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/puissance"
        android:layout_alignBottom="@+id/puissance"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/dateimmat"
        android:layout_alignBottom="@+id/dateimmat"
        android:layout_alignStart="@+id/textView7"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="5dp"
        android:text="Date d'immatriculation :"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="ModÃ¨le :"
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/tv_modele"
        android:layout_alignBottom="@+id/tv_modele"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="Marque :"
        android:textAlignment="center"
        android:layout_alignBaseline="@+id/tv_marque"
        android:layout_alignBottom="@+id/tv_marque"
        android:layout_alignStart="@+id/textView7" />

    <TextView
        android:id="@+id/tv_marque"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="value"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="334dp"
        android:layout_below="@+id/tv_modele"
        android:layout_alignStart="@+id/tv_modele"
        android:layout_marginTop="41dp" />

    <TextView
        android:id="@+id/tv_modele"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/dateimmat"
        android:layout_below="@+id/dateimmat"
        android:layout_marginTop="20dp"
        android:text="value"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="365dp" />

    <TextView
        android:id="@+id/puissance"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/tv_immatriculation"
        android:layout_below="@+id/tv_immatriculation"
        android:layout_marginTop="20dp"
        android:text="value"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="411dp" />

    <TextView
        android:id="@+id/dateimmat"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="value"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="451dp"
        android:layout_marginTop="37dp"
        android:layout_below="@+id/puissance"
        android:layout_alignStart="@+id/puissance" />

</RelativeLayout>
