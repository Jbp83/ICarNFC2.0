        android:text="Marque : "
        android:textAlignment="center"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="334dp" />

    <TextView
        android:id="@+id/modele"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
